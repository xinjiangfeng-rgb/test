package com.xwtech.xwecp.service.logic.pojo;

/**
 * 判断用户是否欠费 判断用户的余额
 */
public class QryUserOwesFee {

    private String isOweFeeHis;//是否欠费

    private String balance;//用户余额

    public String getIsOweFeeHis() {
        return isOweFeeHis;
    }

    public void setIsOweFeeHis(String isOweFeeHis) {
        this.isOweFeeHis = isOweFeeHis;
    }

    public String getBalance() {
        return balance;
    }

    public void setBalance(String balance) {
        this.balance = balance;
    }
}
