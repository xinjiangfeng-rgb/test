package com.xwtech.xwecp.service.logic.pojo;

import java.io.Serializable;

/**
 * 错误编码Bean
 * @author yuantao
 * 2009-12-17
 */
public class ErrorMapping implements Serializable
{
	//逻辑接口编码
	private String liNumber;
	
	//BOSS接口编码
	private String bossIntNum;
	
	//BOSS返回错误码
	private String bossErrCode;
	
	//BOSS返回的错误信息
	private String bossErrMsg;
	
	//逻辑接口返回错误码
	private String liErrCode;
	
	//逻辑接口返回错误信息
	private String liErrMsg;

	public String getBossErrCode() {
		return bossErrCode;
	}

	public void setBossErrCode(String bossErrCode) {
		this.bossErrCode = bossErrCode;
	}

	public String getBossErrMsg() {
		return bossErrMsg;
	}

	public void setBossErrMsg(String bossErrMsg) {
		this.bossErrMsg = bossErrMsg;
	}

	public String getBossIntNum() {
		return bossIntNum;
	}

	public void setBossIntNum(String bossIntNum) {
		this.bossIntNum = bossIntNum;
	}

	public String getLiErrCode() {
		return liErrCode;
	}

	public void setLiErrCode(String liErrCode) {
		this.liErrCode = liErrCode;
	}

	public String getLiErrMsg() {
		return liErrMsg;
	}

	public void setLiErrMsg(String liErrMsg) {
		this.liErrMsg = liErrMsg;
	}

	public String getLiNumber() {
		return liNumber;
	}

	public void setLiNumber(String liNumber) {
		this.liNumber = liNumber;
	}
}
