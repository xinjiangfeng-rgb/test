package com.xwtech.xwecp.service.logic.pojo;

/**
 * 移动之家2018成员信息查询接口
 */
public class MemberInfo {

    private String roleId;//角色ID
    private String roleName;//角色名称
    private String billId;//成员号码
    private String shortType;//短号开通标志
    private String shortBill;//短号
    private String userRegionId;//号码所在地市
    private String groupRegionId;//户主所在地市
    private String expireDate;//失效时间
    private String validDate;//生效时间

    public String getExpireDate() {
        return expireDate;
    }

    public void setExpireDate(String expireDate) {
        this.expireDate = expireDate;
    }

    public String getValidDate() {
        return validDate;
    }

    public void setValidDate(String validDate) {
        this.validDate = validDate;
    }

    public String getRoleId() {
        return roleId;
    }

    public void setRoleId(String roleId) {
        this.roleId = roleId;
    }

    public String getRoleName() {
        return roleName;
    }

    public void setRoleName(String roleName) {
        this.roleName = roleName;
    }

    public String getBillId() {
        return billId;
    }

    public void setBillId(String billId) {
        this.billId = billId;
    }

    public String getShortType() {
        return shortType;
    }

    public void setShortType(String shortType) {
        this.shortType = shortType;
    }

    public String getShortBill() {
        return shortBill;
    }

    public void setShortBill(String shortBill) {
        this.shortBill = shortBill;
    }

    public String getUserRegionId() {
        return userRegionId;
    }

    public void setUserRegionId(String userRegionId) {
        this.userRegionId = userRegionId;
    }

    public String getGroupRegionId() {
        return groupRegionId;
    }

    public void setGroupRegionId(String groupRegionId) {
        this.groupRegionId = groupRegionId;
    }

    @Override
    public String toString() {
        return "QRY180706Result{" +
                "roleId='" + roleId + '\'' +
                ", roleName='" + roleName + '\'' +
                ", billId='" + billId + '\'' +
                ", shortType='" + shortType + '\'' +
                ", shortBill='" + shortBill + '\'' +
                ", userRegionId='" + userRegionId + '\'' +
                ", groupRegionId='" + groupRegionId + '\'' +
                ", expireDate='" + expireDate + '\'' +
                ", validDate='" + validDate + '\'' +
                '}';
    }
}
