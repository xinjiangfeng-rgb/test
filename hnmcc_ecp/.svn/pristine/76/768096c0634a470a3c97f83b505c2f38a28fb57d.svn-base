package com.xwtech.xwecp.service.logic.pojo;

/**
 * 账户入账明细
 */
public class AccountBooks {

    private String payDate;//时间
    private String payedPath;//方式
    private String balanceType;//类别  返还、赠送、预存、充值；
    private String amount;//金额
    private String remarks;//备注 预存或专款名称、营销活动名称或分摊预存名称，分摊次数
    private String payedPathName;//充值渠道

    public String getPayDate() {
        return payDate;
    }

    public void setPayDate(String payDate) {
        this.payDate = payDate;
    }

    public String getPayedPath() {
        return payedPath;
    }

    public void setPayedPath(String payedPath) {
        this.payedPath = payedPath;
    }

    public String getBalanceType() {
        return balanceType;
    }

    public void setBalanceType(String balanceType) {
        this.balanceType = balanceType;
    }

    public String getAmount() {
        return amount;
    }

    public void setAmount(String amount) {
        this.amount = amount;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks;
    }

    public String getPayedPathName() {
        return payedPathName;
    }

    public void setPayedPathName(String payedPathName) {
        this.payedPathName = payedPathName;
    }

    @Override
    public String toString() {
        return "AccountBooks{" +
                "payDate='" + payDate + '\'' +
                ", payedPath='" + payedPath + '\'' +
                ", balanceType='" + balanceType + '\'' +
                ", amount='" + amount + '\'' +
                ", remarks='" + remarks + '\'' +
                ", payedPathName='" + payedPathName + '\'' +
                '}';
    }
}
